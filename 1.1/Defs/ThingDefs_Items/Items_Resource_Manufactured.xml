<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="MedicineBase">
    <defName>VFEV_MedicineNordic</defName>
    <label>nordic medicine</label>
    <description>A pack of herbal concoctions extracted from the healroot plant in combination with honey to boost immunity. It is less potent than industrial pharmaceuticals, but much better than nothing.</description>
    <graphicData>
      <texPath>Things/Item/Resource/Medicine/MedicineNordic</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
      <drawSize>0.85</drawSize>
    </graphicData>
    <techLevel>Neolithic</techLevel>
    <statBases>
      <MarketValue>14</MarketValue>
      <Mass>0.35</Mass>
      <Flammability>1.3</Flammability>
      <MedicalPotency>0.75</MedicalPotency>
      <MedicalQualityMax>0.85</MedicalQualityMax>
      <DeteriorationRate>7</DeteriorationRate>
      <WorkToMake>500</WorkToMake>
    </statBases>
    <costList>
      <MedicineHerbal>1</MedicineHerbal>
      <VFEV_Honey>5</VFEV_Honey>
    </costList>
    <recipeMaker>
      <workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
      <workSkill>Intellectual</workSkill>
      <recipeUsers>
        <li>DrugLab</li>
      </recipeUsers>
      <skillRequirements>
        <Crafting>2</Crafting>
        <Intellectual>2</Intellectual>
      </skillRequirements>
    </recipeMaker>
    <tickerType>Rare</tickerType>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>150</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
  </ThingDef>
  
</Defs>
